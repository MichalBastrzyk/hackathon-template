DATABASE_URL=
DATABASE_AUTH_TOKEN=

# Better Auth Configuration
BETTER_AUTH_SECRET=
BETTER_AUTH_URL=http://localhost:3000

# S3-Compatible Storage Configuration
# For MinIO (local development): http://localhost:9000
# For Cloudflare R2: https://<account-id>.r2.cloudflarestorage.com
# For Hetzner: https://<region>.your-objectstorage.com
S3_ENDPOINT=http://localhost:9000
S3_REGION=auto
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET_NAME=uploads
# Optional: Public URL for accessing uploaded files (defaults to S3_ENDPOINT if not set)
S3_PUBLIC_URL=http://localhost:9000
